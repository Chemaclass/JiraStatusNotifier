{
    "name": "chemaclass/scrum-master",
    "description": "This tool will notify the person assigned to a JIRA-ticket if the ticket remains in the same status for more than N days.",
    "type": "library",
    "homepage": "https://github.com/Chemaclass/ScrumMaster",
    "license": "MIT",
    "keywords": [
        "agile",
        "jira",
        "jira-api",
        "notifier",
        "php",
        "reminder",
        "scrum",
        "scrum-master",
        "slack",
        "slack-api",
        "slack-bot"
    ],
    "authors": [
        {
            "name": "Jose Maria Valera Reales",
            "email": "chemaclass@outlook.es",
            "homepage": "http://chemaclass.es/"
        },
        {
            "name": "Jesus Valera Reales",
            "email": "jesus1902@outlook.com"
        }
    ],
    "require": {
        "ext-json": "*",
        "php": "^7.2",
        "symfony/http-client": "^4.3",
        "symfony/mailer": "^5.0",
        "twig/twig": "^3.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.15",
        "infection/infection": "^0.14.2",
        "phpunit/phpunit": "^8.5.0",
        "symfony/var-dumper": "^4.3",
        "symfony/google-mailer": "^5.0",
        "twig/twig": "^3.0",
        "vlucas/phpdotenv": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "Chemaclass\\ScrumMaster\\": "src/ScrumMaster"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Chemaclass\\ScrumMasterTests\\": "tests/ScrumMasterTest"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
