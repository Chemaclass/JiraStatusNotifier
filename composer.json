{
    "name": "chemaclass/ScrumMaster",
    "description": "This tool will notify the person assigned a JIRA-ticket if the ticket remains in the same status for more than N days.",
    "type": "library",
    "keywords": [
        "jira",
        "slack",
        "bot",
        "jira-api",
        "slack-api",
        "php",
        "scrum",
        "scrum-master"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Jose Maria Valera Reales",
            "email": "chemaclass@outlook.es"
        }
    ],
    "require": {
        "php": "^7.3",
        "ext-json": "*",
        "vlucas/phpdotenv": "^3.6",
        "symfony/http-client": "^4.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.15",
        "phpunit/phpunit": "^8.4",
        "symfony/var-dumper": "^4.3",
        "infection/infection": "^0.14.2"
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "src/",
            "App\\Tests\\": "tests/"
        }
    }
}
